{
  "name": "roamjs-scripts",
  "description": "Description for roamjs-scripts",
  "version": "0.2.15",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "cross-env NODE_ENV=test npm t",
    "build": "ncc build src/index.ts -o dist",
    "postbuild": "node postbuild.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "prepublishOnly": "npm run build",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push origin main && git push --tags",
    "pretest": "npm run lint",
    "test": "jest --config jestconfig.json",
    "posttest": "rimraf build"
  },
  "license": "MIT",
  "files": [
    "/dist"
  ],
  "bin": {
    "roamjs-scripts": "./dist/index.js"
  },
  "devDependencies": {
    "@types/dotenv-webpack": "^7.0.1",
    "@types/git-repo-name": "^1.0.0",
    "@types/jest": "^26.0.23",
    "@types/mini-css-extract-plugin": "^1.4.2",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@vercel/ncc": "^0.28.5",
    "cross-env": "^7.0.3",
    "eslint": "^7.25.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.5",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.13.13",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.4",
    "dotenv": "^8.4.0",
    "file-loader": "^6.2.0",
    "git-repo-name": "^1.0.1",
    "nearley-loader": "^2.0.0",
    "shebang-loader": "0.0.1",
    "style-loader": "^2.0.0",
    "svg-react-loader": "^0.4.6",
    "ts-loader": "^8.2.0",
    "url-loader": "^4.1.1",
    "webpack": "^4.44.1"
  }
}
